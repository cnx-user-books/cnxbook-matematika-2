                    </m:msup>
                    <m:mrow>
                      <m:mi>x</m:mi>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mi>f</m:mi>
                    </m:mrow>
                    <m:mo stretchy="false">(</m:mo>
                    <m:mi>z</m:mi>
                    <m:mrow>
                      <m:mo stretchy="false">)</m:mo>
