                    </m:mrow>
                    <m:mi>C</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ Int { {  { ital "dy"}  over  {y} } } = Int { {  { ital "dx"}  over  {x} } } +"ln"C} {}</m:annotation>
