                            </m:mstyle>
                            <m:mi>x</m:mi>
                          </m:mfrac>
                        </m:mrow>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mtext>ln</m:mtext>
                      </m:mrow>
                    </m:mrow>
                    <m:mi>C</m:mi>
                    <m:mi>,</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mrow/>
