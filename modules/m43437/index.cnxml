              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {  { ital "dy"}  over  { ital "dx"} } =f left ( {  {y}  over  {x} }  right ) "." } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para><para id="import-auto-id1163649992938"> Со воведување на смената</para>
        <para id="import-auto-id1163649777152">
          <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
            <m:semantics>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:mfrac>
                      <m:mi>y</m:mi>
                      <m:mi>x</m:mi>
                    </m:mfrac>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mi>z</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {  {y}  over  {x} } =z} {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para id="import-auto-id1163649572176"> односно </para>
        <para id="import-auto-id1163649294889">
          <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
            <m:semantics>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:mi>y</m:mi>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mstyle fontstyle="italic">
