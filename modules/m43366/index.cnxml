                        <m:msup>
                          <m:mi>e</m:mi>
                          <m:mrow>
                            <m:mrow>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mi>x</m:mi>
                            </m:mrow>
                          </m:mrow>
