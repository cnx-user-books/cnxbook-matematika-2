                        </m:mrow>
                      </m:msup>
                      <m:mo stretchy="false">+</m:mo>
                      <m:mi/>
                    </m:mrow>
                    <m:mtext>.</m:mtext>
                    <m:mi/>
                    <m:mtext>.</m:mtext>
                    <m:mi/>
                    <m:mtext>.</m:mtext>
                    <m:mrow>
                      <m:mi/>
                      <m:mo stretchy="false">+</m:mo>
                      <m:msub>
