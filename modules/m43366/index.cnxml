              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ { {y}} sup { ' } { {y}} sup { ''' }=3 \(  { {y}} sup { '' } \)  rSup { size 8{2} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para xmlns:m="http://www.w3.org/1998/Math/MathML" id="import-auto-id1166579590643"><m:math>
            <m:semantics>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:msup>
                      <m:mi>x</m:mi>
                      <m:mrow>
                        <m:mn>2</m:mn>
                      </m:mrow>
                    </m:msup>
                    <m:mi>y</m:mi>
                    <m:mrow>
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:msup>
                              <m:mi>d</m:mi>
                              <m:mrow>
                                <m:mn>2</m:mn>
                              </m:mrow>
                            </m:msup>
                            <m:mi>y</m:mi>
                          </m:mrow>
                          <m:mstyle fontstyle="italic">
                            <m:mrow>
                              <m:msup>
                                <m:mtext>dx</m:mtext>
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:msup>
                            </m:mrow>
                          </m:mstyle>
                        </m:mfrac>
                        <m:mo stretchy="false">−</m:mo>
                        <m:msup>
                          <m:mfenced open="(" close=")">
                            <m:mrow>
                              <m:mi>x</m:mi>
                              <m:mrow>
                                <m:mfrac>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>dy</m:mtext>
