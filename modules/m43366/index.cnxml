                      </m:mrow>
                    </m:mstyle>
                  </m:mrow>
                </m:mrow>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ \( 1 -  ital "xy"+x rSup { size 8{2} } y rSup { size 8{2} }  \)  ital "dx"=x rSup { size 8{2} }  ital "dy"} {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para id="import-auto-id1166576481935">
          <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
            <m:semantics>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:msup>
                      <m:mi>y</m:mi>
